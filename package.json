    "express-fileupload": "^0.1.3",
    "filenamify": "^2.0.0",
    "graphql": "^0.10.3",
    "graphql-server-express": "^0.9.0",
    "graphql-tools": "^1.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pbn": "1.x",
    "pg": "^6.4.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
